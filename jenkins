pipeline {
    agent any

    stages {
        stage("Checkout Code") {
            steps {
                git url: 'https://github.com/mudunuri010/git-documentation.git'
            }
        }

        stage("Build") {
            steps {
                bat 'echo Building the app'
            }
        }

        stage("Test") {
            steps {
                bat 'echo Testing the app'
            }
        }

        stage("Deploy") {
            steps {
                bat 'echo Deploying the app'
            }
        }
    }
}
post{
  success{
    bat 'echo  Build Successfully'
}
failure{
    bat 'echo Build Unsuccessfully'
   }
}

   

