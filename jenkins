pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building the Application'
                sh "npm run dev"
            }
        }
        stage('Test'){
            steps{
                echo 'Running the  Tests'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying the Tests'
            }
        }
    }
}
